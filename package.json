{
  "name": "react-chatbotify-gallery-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express-session": "^1.18.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.1.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"types/**/*.ts\"",
    "build": "npx tsc --build",
    "lint": "eslint \"{src,test,types}/**/*.ts\" --fix",
    "deploy-prod": "docker compose -p prod -f ./docker/docker-compose.yml down && docker compose -p prod -f ./docker/docker-compose.yml up --build -d",
    "stop-prod": "docker compose -p prod -f ./docker/docker-compose.yml down",
    "deploy-playground": "docker compose -p playground -f ./docker/docker-compose.playground.yml down && docker compose -p playground -f ./docker/docker-compose.playground.yml up --build -d",
    "stop-playground": "docker compose -p playground -f ./docker/docker-compose.playground.yml down",
    "dev": "docker compose -p dev -f ./docker/docker-compose.dev.yml down && docker compose -p dev -f ./docker/docker-compose.dev.yml up --build",
    "dev:watch-api": "nodemon --config nodemon-api.json",
    "dev:watch-jobs": "nodemon --config nodemon-jobs.json"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "axios": "^1.8.2",
    "chromadb": "^1.8.1",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "minio": "^8.0.1",
    "multer": "^2.0.1",
    "mysql2": "^3.9.4",
    "nodemon": "^3.1.0",
    "openai": "^4.33.0",
    "redis": "^4.6.13",
    "request": "^2.88.2",
    "sequelize": "^6.37.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.14.2"
  }
}
